{
    "manifest_version": 2,
    "name": "Umbraco Productivity",
    "short_name": "Umbraco",
    "description": "Productivity for Umbraco developers",
    "author": "Umbraco HQ",
    "version": "1.0.0",
    "options_page": "options.html",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
